package BasicJavaPrograms;

public class ForLoop2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		  String s1="Hello Hi";
		  int len = s1.length();
		  
		  for(int i=2;i<len;i++)
		  {
			  //System.out.print(i);
			  System.out.println(s1.charAt(i));
			  
		  }
	}

} 
